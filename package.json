{
    "name": "git-flow-action",
    "version": "1.0.1",
    "description": "Github action to handle git flow",
    "main": "lib/main/index.js",
    "scripts": {
        "build": "npm-run-all build:tsc build:ncc",
        "build:tsc": "tsc",
        "build:ncc": "ncc build",
        "pre-push": "node ./scripts/pre-push.js"
