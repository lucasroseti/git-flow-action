    "license": "MIT",
    "bugs": {
        "url": "https://github.com/VAEES/git-flow-action/issues"
    },
    "homepage": "https://github.com/VAEES/git-flow-action#readme",
    "dependencies": {
        "@actions/core": "^1.2.6",
        "@actions/github": "^4.0.0"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^4.14.2",
        "@typescript-eslint/parser": "^4.14.2",
        "@vercel/ncc": "^0.38.1",
        "eslint": "^7.19.0",
        "eslint-config-google": "^0.14.0",
        "husky": "^4.3.8",
        "lint-staged": "^10.5.3",
        "npm-run-all": "^4.1.5",
        "typescript": "^4.1.3"
    }
